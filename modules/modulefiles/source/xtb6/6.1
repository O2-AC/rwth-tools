# For the local install (if installed in HOME)
set module_base_path "$::env(HOME)/local/modules/modulefiles/source"

# Needs to be adjusted for what is to be loaded
set MAJORVERSION "6"
set MINORVERSION "1"

# Will be read by the source module file
